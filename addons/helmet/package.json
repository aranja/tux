{
  "name": "tux-addon-helmet",
  "version": "1.0.4",
  "description": "Use helmet for server and client side metadata.",
  "bugs": {
    "url": "https://github.com/aranja/tux/issues"
  },
  "repository": "aranja/tux",
  "keywords": [
    "react",
    "react-helmet",
    "tux-addon"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepublish": "NODE_ENV=production ../../tasks/build-package.js",
    "watch": "../../tasks/build-package.js --watch",
    "test": "../../node_modules/.bin/jest"
  },
  "peerDependencies": {
    "react": ">16.0.0-beta",
    "react-chain": "^0.5.0",
    "react-helmet": "^5.0.1"
  },
  "files": [
    "es",
    "lib"
  ],
  "license": "MIT",
  "devDependencies": {
    "jest": "^18.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/../../shared/jest-preprocessor"
    },
    "testRegex": "/(src|test).*(\\.|/)(test|spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
