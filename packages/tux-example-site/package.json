{
  "name": "tux-example-site",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "npm-run-all": "^4.0.2",
    "redbox-react": "^1.3.4"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.5.1",
    "neutrino": "^4.2.1",
    "neutrino-preset-tux": "0.1.0",
    "pretty-error": "^2.0.2",
    "react-helmet": "^4.0.0",
    "tux": "^0.1.0",
    "tux-adapter-contentful": "^0.1.0",
    "universal-router": "^2.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "npm run start:client",
    "build": "npm run build:client",
    "start-server": "npm-run-all --parallel build:client start:server && node build/server",
    "build-server": "npm-run-all --parallel build:client build:server",
    "start:client": "PORT=3000 HTTPS=true neutrino start --presets neutrino-preset-tux",
    "build:client": "neutrino build --presets neutrino-preset-tux",
    "start:server": "neutrino start --presets neutrino-preset-tux/server",
    "build:server": "neutrino build --presets neutrino-preset-tux/server"
  }
}
